#!/usr/bin/env python3
import sqlite3, os
# ---- ここを書き換えてください ----
BACKUP_DIR = os.path.expanduser(
    "~/Library/Application Support/MobileSync/Backup/00008120-0004048C3C42201E"
)
DB_PATH = "Manifest.db"
# ----------------------------------
conn = sqlite3.connect(DB_PATH)
cur = conn.cursor()
# ファイルレコードを走査
cur.execute("SELECT fileID FROM Files")
to_delete = []
for (fileID,) in cur:
    # 実ファイルは <prefix 2 文字>/<fileID> に格納されている
    p = os.path.join(BACKUP_DIR, fileID[:2], fileID)
    if not os.path.exists(p):
        to_delete.append(fileID)
print(f"削除対象エントリ: {len(to_delete)} 件")
if to_delete:
    # トランザクションでまとめて削除
    cur.executemany("DELETE FROM Files WHERE fileID = ?", [(fid,) for fid in to_delete])
    conn.commit()
    print("削除完了。データベースを最適化 (VACUUM) します…")
    cur.execute("VACUUM;")
    conn.close()
    print("完了しました。")
else:
    print("削除するエントリはありませんでした。")
    conn.close()
import sympy as sp
a, b, c = sp.Symbol("a b c")
import sympy as sp
